{% layout 'layout' %}
{% block rel %}
  <h1>collection: {{collection}}</h1>
  <section>
    <h2>items</h2>
    <ul>
      {% for item in api.response.body[collection] %}
        <li>
          <a href="/streamlabs/{{collection}}/{{item[identifier]}}">
            {{ item[identifier] }}</a>
        </li>
      {% endfor %}
    </ul>
  </section>
{% endblock %}

{% block pen %}
{% include 'resource' %}
{% endblock %}
